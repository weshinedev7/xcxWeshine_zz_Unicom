<!-- 选项卡 -->
<view class="display">
  <view class="display-sele {{check=='0' ? 'active' : ''}}" bindtap='selectDispaly' data-id="0">
    全部
  </view>
  <view class="display-sele {{check=='1' ? 'active' : ''}}" bindtap='selectDispaly' data-id="1">
    未通过
  </view>
  <view class="display-sele {{check=='2' ? 'active' : ''}}" bindtap='selectDispaly' data-id="2">
    已通过
  </view>
</view>

<!-- 内容 -->
<view wx:if="{{check=='0'}}" class="eval-all">
  <view class='items'>
    <view class="flex-row item">
      <view class="left ">会议标题</view>
      <view class="right more">人事人员培训</view>
    </view>
    <view class="flex-row item">
      <view class="left">开始时间</view>
      <view class="right">2019-04-28 13:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">结束时间</view>
      <view class="right">2019-04-28 15:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">会议人数</view>
      <view class="right">5</view>
    </view>
    <view class="flex-row item">
      <view class="left">会议内容</view>
      <view class="right more">人员信息要明确，做好人员的规划。做好公司的小助手</view>
    </view>
    <view class="flex-row item">
      <view class="left">补充选项</view>
      <view class="right">茶水、鲜花</view>
    </view>
    <view class="flex-row item">
      <view class="left">备注</view>
      <view class="right">请提前检查好投影仪</view>
    </view>

    <!--判断是否通过审批-->
    <view class='flex-row flr'>
      <navigator url="/pages/makeAnAppointment/makeAnAppointment" hover-class="none" class="edit">修改</navigator>
      <view class='cancel'>取消</view>
    </view>
  </view>

  <view class='items'>
    <view class="flex-row item">
      <view class="left ">会议标题</view>
      <view class="right more">人事人员培训</view>
    </view>
    <view class="flex-row item">
      <view class="left">开始时间</view>
      <view class="right">2019-04-28 13:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">结束时间</view>
      <view class="right">2019-04-28 15:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">会议人数</view>
      <view class="right">5</view>
    </view>
    <view class="flex-row item">
      <view class="left">会议内容</view>
      <view class="right more">人员信息要明确，做好人员的规划。做好公司的小助手</view>
    </view>
    <view class="flex-row item">
      <view class="left">补充选项</view>
      <view class="right">茶水、鲜花</view>
    </view>
    <view class="flex-row item">
      <view class="left">备注</view>
      <view class="right">请提前检查好投影仪</view>
    </view>
    <view class='status_yes'>
      <view class="flex-row">
        <view>审批人:</view>
        <view class="l30">admin</view>
      </view>
      <view class='flex-row'>
        <view>会议室:</view>
        <view class="l30">四楼4#123</view>
      </view>
      <view class='flex-row'>
        <view>审批时间:</view>
        <view class="l30">2019-04-28 12:40:00</view>
      </view>
      <view class='flex-row flr'>
        <view class='yes'>已通过</view>
      </view>
    </view>
  </view>
</view>

