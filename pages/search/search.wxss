/* pages/search/search.wxss */
/*头部*/
.top{
	width:100%;
	height:90rpx;
}
.search-block {
  margin: 10rpx 30rpx;
  display: block;
}
.search-block navigator {
  display: block;
  background: #eee;
  text-align: left;
  height: 50rpx;
  line-height: 70rpx;
  border: 10rpx solid #eee;
  border-radius: 30rpx;
  color: #b2b2b2;
  position: relative;
  display:flex; 
  align-items:center;

}
.search-block navigator image {
  height: 30rpx;
  width: 30rpx;
  margin: 0 10rpx;
}
.search-block navigator text{
	font-size:30rpx;
	margin: 0 10rpx;
}


.history{
	min-height:250rpx;
	margin:30rpx;
	
}

.history-text{
    font-size:25rpx;
	color:rgb(156, 151, 151);
	margin:10rpx 0rpx;
}

.history-button{
    width:100rpx;
	height:50rpx;
	font-size:25rpx;
	line-height: 50rpx;
	float:left;
	text-align:center;
	border-radius: 20rpx;
	margin:8rpx;
	border:1rpx solid #a8a8a8;
	}