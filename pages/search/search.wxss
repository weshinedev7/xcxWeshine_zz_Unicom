/* pages/search/search.wxss */
/*头部*/
.top{
	width:100%;
	height:90rpx;
}
.search-block {
    margin: 10rpx 30rpx;
    display: block;
}
.search-block navigator {
  display: block;
  background: #eee;
  text-align: left;
  height: 50rpx;
  line-height: 70rpx;
  border: 10rpx solid #eee;
  border-radius: 30rpx;
  color: #b2b2b2;
  position: relative;
  display:flex; 
  align-items:center;

}
.search-block navigator image {
  height: 30rpx;
  width: 30rpx;
  margin: 0 10rpx;
}
.search-block navigator text{
	font-size:30rpx;
	margin: 0 10rpx;
}

.search-text{
	color:#1296db;
	font-size:30rpx;
	width:100%;
	height:40rpx;
	text-align:center;
}
