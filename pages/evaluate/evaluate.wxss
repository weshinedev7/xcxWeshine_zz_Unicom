page {
  background: #f3f3f3;
  width: 100%;
}

/* 主体 */

.main {
  width: 100%;
  padding-top: 10rpx;
}

.main_content {
  width: 96%;
  border-radius: 26rpx;
  padding: 20rpx 0rpx;
  margin: 10rpx auto;
  background: white;
  overflow: hidden;
}

.main_left {
  width: 20%;
  float: left;
  height: 150rpx;
}

.main_left image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 20rpx 0rpx 0rpx 20rpx;
}

.main_right {
  width: 79%;
  float: left;
}

.main_right_top {
  width: 100%;
  height: 100rpx;
  padding-bottom: 10rpx;
  border-bottom: 1rpx dotted #d1d0d0;
}

.main_right_top_left {
  width: 50%;
  height: 100rpx;
  float: left;
}

.main_right_top_left view {
  width: 100%;
  height: 33%;
	float:left;
  font-size: 13px;
  color: #838383;
}

.main_right_top_right {
  width: 50%;
  float: left;
  font-size: 13px;
  color: #838383;
}

.main_right_bottom{
	padding-top:10rpx;
}


.main_right_bottom_top {
  width: 100%;
  height: 33%;
	padding-bottom:5rpx;
  font-size: 13px;
  color: #1d1c1c;
}

.main_right_bottom_bottom {
  width: 100%;
  font-size: 13px;
  color: #636262;
}

.middle_none {
  text-align: center;
  font-size: 28rpx;
  color: #838383;
  margin-top: 300rpx;
}
