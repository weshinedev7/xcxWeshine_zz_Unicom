<!-- 选项卡 -->
<view class="display">
  <view class="display-sele {{check=='0' ? 'active' : ''}}" bindtap='selectDispaly' data-id="0">
    全部
  </view>
  <view class="display-sele {{check=='1' ? 'active' : ''}}" bindtap='selectDispaly' data-id="1">
    未通过
  </view>
  <view class="display-sele {{check=='2' ? 'active' : ''}}" bindtap='selectDispaly' data-id="2">
    已通过
  </view>
</view>

<!-- 内容 -->
<view wx:if="{{check=='0'}}" class="eval-all">
  <view class='items' wx:for="{{vehicle}}">
		
		<view class="{{item.status == 0?'status':''}}">{{item.status == 0?'待审核':''}}</view>		
		<view class="{{item.status == 1?'status':''}}">{{item.status == 1?'未通过':''}}</view>		
		<view class="{{item.status == 2?'status':''}}" style="{{item.status == 2?'background:#51c332':''}}">{{item.status == 2?'已通过':''}}</view>
    <view class="flex-row item">
      <view class="left">开始时间</view>
      <view class="right">2019-04-28 13:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">结束时间</view>
      <view class="right">2019-04-28 15:30:00</view>
    </view>
    <view class="flex-row item">
      <view class="left">乘坐人数</view>
      <view class="right">5</view>
    </view>
    <view class="flex-row item">
      <view class="left">用途</view>
      <view class="right more">购买公司的团建活动的材料</view>
    </view>
		
    <!--判断是否通过审批-->
    <view class='{{item.status == 0?"flex-row flr":""}}'>
      <navigator url="/pages/bookingVehicle/bookingVehicle" hover-class="none" class="{{item.status == 0?'edit':''}}">{{item.status == 0?"修改":""}}</navigator>
      <view class="{{item.status == 0?'cancel':''}}">{{item.status == 0?"取消":""}}</view>
    </view>

		<view class='{{item.status == 1?"status_yes":""}}' wx:if="{{item.status == 1}}">
      <view class="tac">审核结果</view>
      <view class="flex-row">
        <view class="left">审批人:</view>
        <view class="l30 pass">{{item.approver}}</view>
      </view>
      <view class='flex-row'>
        <view class="left">取消原因:</view>
        <view class="l30 pass">{{item.reason}}</view>
      </view>
      <view class='flex-row'>
        <view class="left">审批时间:</view>
        <view class="l30 pass">{{item.date}}</view>
      </view>
    </view>

		<view class='{{item.status == 2?"status_yes":""}}' wx:if="{{item.status == 2}}">
      <view class="tac">审核结果</view>
      <view class="flex-row">
        <view class="left">审批人:</view>
        <view class="l30 pass">{{item.approver}}</view>
      </view>
      <view class='flex-row'>
        <view class="left">车牌号:</view>
        <view class="l30 pass">{{item.vehicle_id}}</view>
      </view>
      <view class='flex-row'>
        <view class="left">审批时间:</view>
        <view class="l30 pass">{{item.date}}</view>
      </view>
    </view>

  </view>

</view>

