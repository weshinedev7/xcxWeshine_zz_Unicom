/* 顶部搜索 */
/*头部*/
.top{
			width:100%;
			height:90rpx;
}
.search-block {
  		margin: 10rpx 30rpx;
  		display: block;
}
.search-block navigator {
			background: #eee;
			text-align: left;
			height: 50rpx;
			line-height: 70rpx;
			border: 10rpx solid #eee;
			border-radius: 30rpx;
			color: #b2b2b2;
			position: relative;
			align-items:center;

}

.search-block navigator image {
			height: 30rpx;
			width: 30rpx;
			position:relative;
			top:-6rpx;
			left:20rpx;
}
.search-block navigator text{
			font-size:30rpx;
			margin: 0 10rpx;
			position:relative;
			top:-12rpx;
			left:20rpx;
}

/* 条件选择 */
.date{
		width:90%;
		height:60rpx;
		margin:0rpx auto;
		padding: 10rpx;
}
		
.date_title{
		width:90%;
		height:60rpx;
		margin:0rpx auto;
		border-bottom:1rpx dotted #838383;
}

.date_start{width:50%;
		height: 60rpx;
		font-size:25rpx;
		float:left;
		text-align: center;
		line-height: 60rpx;
		}

.date_end{width:50%;
		height: 60rpx;
		font-size:25rpx;
		float:left;
		text-align: center;
		line-height: 60rpx;
		}

