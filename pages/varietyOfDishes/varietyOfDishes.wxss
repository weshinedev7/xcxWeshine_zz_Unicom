/*头部*/
.top{
	width:85%;
	height:90rpx;
	float:left;
}
.search-block {
  margin: 10rpx 30rpx;
  display: block;
}
.search-block .navigator {
  display: block;
  background: #eee;
  text-align: left;
  height: 50rpx;
  line-height: 70rpx;
  border: 10rpx solid #eee;
  border-radius: 30rpx;
  color: #b2b2b2;
  position: relative;
  display:flex; 
  align-items:center;

}
.search-block .navigator image {
  height: 30rpx;
  width: 30rpx;
  margin: 0 10rpx;
}
.search-block .navigator text{
	font-size:30rpx;
	margin: 0 10rpx;
}

.add{width:11%;
	height:90rpx;
	float:left;
	margin-top:10rpx;
}
.add image{width:100%;
	height:80rpx;
}

.display{
	width:100%;
	display: flex;
	flex-direction:row;
	margin-top:20rpx;
}
.display-sele{
    font-size:30rpx;
	width:33.3%;
	text-align:center;
	border-bottom: 1px solid #838383;
	height:50rpx;
}

.active{
	color: #dd2727;
	border-bottom: 1px solid #dd2727;
}

/* 预约会议室 */
.main{min-height:180rpx;
				width:90%;
				margin:20rpx auto 0rpx;
				border-bottom:1rpx solid rgb(241, 235, 235);
				}

.content_left_content{
				/* width:220rpx; */
				width:32%;
				float:left;
				}

.centent-img{width:180rpx;
				height:130rpx;
				border-radius: 20rpx;
				margin:20rpx 0rpx 0rpx 20rpx;
				}


.content_left_right{width:55%;
				height:130rpx;
				/* position: relative; */
				padding:0rpx 10rpx;
				margin-top:20rpx;
				float:left;

				overflow: hidden;
  			text-overflow: ellipsis;
  			white-space: nowrap
				}

.content_name{font-size:26rpx;
				font-weight: 900;
				margin-top:10rpx;				
				overflow: hidden;
  			text-overflow: ellipsis;
  			white-space: nowrap;
				}

.introduction{
				font-size:23rpx;
				margin-top:5rpx;
				overflow: hidden;
  			text-overflow: ellipsis;
  			white-space: nowrap
			}

.frequency{font-size:20rpx;
				width:150rpx;
				margin-top:5rpx;
				color:#838383;
				line-height: 36rpx;
				float:left;
				}

.operation{
			width:10%;
			height:130rpx;
			float:left;
			margin-top:15rpx;
			}

/* .operation image{
			width:20rpx;
			height:30rpx;
			} */

.operation navigator{padding:0rpx;
			width:60rpx;
			height:40rpx;
			border:none;
			font-size:25rpx;
			margin:15rpx 0rpx;
			text-align: right;
			}

.operation .edit{
			width:80%;
			height:40rpx;
			border:none;
			background: #dd2727;
			font-size:23rpx;
			color:white;
			border-radius: 10rpx;
			line-height: 40rpx;
			margin:15rpx auto;
			text-align: center;
			}



.page-top image{width:20rpx;
	height:20rpx;}

.page-bottom{
				position:absolute;
				width:500rpx;
				height:500rpx;
				background: red;
				display:none;}


.c-state1{
  transform: rotate(0deg) scale(1) translate(75%,0%); 
  -webkit-transform: rotate(0deg) scale(1) translate(75%,0%); 
}




