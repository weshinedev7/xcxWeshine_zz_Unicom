  position: relative;
  background: white;
	margin-bottom:10rpx;
}

.top_img {
  width: 100%;
  height: 140rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  -webkit-filter: blur(15px);
  -moz-filter: blur(15px);
  -o-filter: blur(15px);
  -ms-filter: blur(15px);
  filter: blur(15px);
}


.top_img image {
  width: 100%;
  height: 100%;
}

.store_img {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: 50rpx;
  left: 40%;
}

.store {
  width: 100%;
  height: 200rpx;
  margin-top: 80rpx;
}

.store_name {
  width: 60%;
  height: 50rpx;
  margin: 0rpx auto;
}

.store_name_content {
  width: 100%;
  height: 50rpx;
  float: left;
  text-align: center;
  line-height: 50rpx;
  font-weight: 700;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.strat{
  width: 36rpx;
  height: 36rpx;
  margin-left:4rpx;
}


.edit_img {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
  margin-top: 13rpx;
  float: left;
}

.store_text {
  height: 50rpx;
  margin: 0rpx auto;
  line-height: 50rpx;
}

.store_text .store_comment {
  width: 100%;
  float: left;
  font-size: 12px;
  text-align: center;
  color: #838383;
}

