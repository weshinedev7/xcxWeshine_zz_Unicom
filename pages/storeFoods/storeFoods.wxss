}

/*总体主盒子*/

.container {
  position: relative;
  width: 100%;
  height: 100%;
  color: #939393;
}

/*左侧栏主盒子*/

.nav_left {
  display: inline-block;
  width: 25%;
  background: white;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
}

/*左侧栏list的item*/

.nav_left .nav_left_items {
  height: 40px;
  line-height: 40px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}

/*左侧栏list的item被选中时*/

.nav_left .nav_left_items.active {
  background: #fff;
  color: #dd2727;
  font-weight: 700;
}

/*右侧栏主盒子*/

.nav_right {
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  box-sizing: border-box;
  background: #f3f3f3;
  padding-bottom:100rpx;
}

/*右侧栏list的item*/

.nav_right .nav_right_items {
  float: right;
  width: 98%;
  margin-bottom: 10rpx;
  background: white;
  border-bottom: 1px solid #dedede;
}

/* 菜品头部 */

.food_top {
  width: 100%;
  height: 220rpx;
}

.food_img {
  width: 30%;
  height: 170rpx;
  margin-top: 25rpx;
  margin-left: 15rpx;
  float: left;
}

.food_img image {
  width: 100%;
  height: 170rpx;
}

.food_text {
  width: 65%;
  height: 170rpx;
  margin-top: 25rpx;
  float: right;
}

.food_name,.food_more,.food_brief {
  width: 100%;
  height: 30rpx;
  margin-top: 8rpx;
  color: black;
  font-size: 12px;
}

.food_brief {
  color: #888787;
  font-size: 12px;
}

.sold{
	font-size:12px;
	float:left;
	color:#888787;
  padding:20rpx 10rpx 10rpx 0rpx;
}

.food_name {
  font-weight: 700;
  font-size: 14px;
}


.food_brief {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.operation{
	width:100%;
	height:60rpx;
	/* background: pink; */
  font-size: 12px;
	color:#888787;
  clear:both;
  overflow:hidden;
}

.food_price {
	width:40%;
  color: red;
	font-size:13px;
