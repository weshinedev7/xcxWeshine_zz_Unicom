/*普通样式*/  
.stepper .normal{  
    color: black;  
}  
/*禁用样式*/  
.stepper .disabled{  
    color: #ccc;  
} 


.nodata_text {
  color: black;
  font-size: 14px;
  text-align: center;
  width: 100%;
  line-height: 500rpx;
}

.clear:after {
  content: '';
  display: block;
  visibility: hidden;
  height: 0;
  clear: both;
}

/* 菜品评论选项卡 */

/* 下面是tobar */

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background-color: white;
	margin-bottom:2rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 25%;
