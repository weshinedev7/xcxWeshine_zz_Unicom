<view class="display">
  <view class="display-sele {{check=='0' ? 'active' : ''}}" bindtap='selectDispaly' data-id="0">
    全部
  </view>
  <view class="display-sele {{check=='1' ? 'active' : ''}}" bindtap='selectDispaly' data-id="1">
    自付
  </view>
  <view class="display-sele {{check=='2' ? 'active' : ''}}" bindtap='selectDispaly' data-id="2">
    部门代付
  </view>
</view>

<!--评价-->
<view wx:if="{{check=='0'}}" class="eval-all" wx:for="{{list}}" wx:key="*this">
  <view class="flex-row flex-grow-1 order-info">
    <view class="flex-grow-1 tl">订单号:20190425000000{{item.ordernum}}</view>
    <view class="flex-grow-0 tr">2019-04-25</view>
  </view>
  <view class="goods-list flex-row">
    <image class="goods-pic" mode="aspectFill" src="/images/img2.png" />
    <view class="content flex-grow-1">
      <view class="flex-grow-1">
        <view class="goods-name text-style">五香排骨</view>
        <view class="attr-item">门店1</view>
      </view>
      <view class="flex-grow-1 flex-row">
        <view class="flex-grow-1 num">×2</view>
        <view class="flex-grow-0 price">￥333</view>
      </view>
    </view>
  </view>
  <view class="goods-list flex-row">
    <image class="goods-pic" mode="aspectFill" src="/images/img2.png" />
    <view class="content flex-grow-1">
      <view class="flex-grow-1">
        <view class="goods-name text-style">测试测试</view>
        <view class="attr-item">门店1</view>
      </view>
      <view class="flex-grow-1 flex-row">
        <view class="flex-grow-1 num">×2</view>
        <view class="flex-grow-0 price">￥333</view>
      </view>
    </view>
  </view>
  <view class="flex-row count-order">
    <view class="flex-grow-1 text-style">合计:￥123</view>
    <navigator url="/pages/score/score" hover-class="none" class="flex-grow-0 score">评分</navigator>
  </view>

