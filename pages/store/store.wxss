page{
	background: #f5f5f5;
}
.page{
	height:100%;
	width: 100%;
	padding-top:30rpx; 
}

.store_icon{
	width:100%;
	height:400rpx;
	padding-top:100rpx;
}

.icon_content{
	width: 200rpx;
	height:200rpx;
	margin:0rpx auto;
	border-radius: 50%;
}

.submit{
	width: 23%;
	height:60rpx;
	line-height: 60rpx;
	font-size:14px;
	margin:50rpx auto 0rpx;
}

.content{
	width:100%;
	height:90rpx;
	background: red;
	margin:0rpx auto;
	background: white;
	margin-bottom:10rpx;
	border-bottom:1rpx solid #f8f8f8;
}

.content_input{
	width:95%;
	height:90rpx;
	margin:0rpx auto;
}

.input_text{
	width:21%;
	height:90rpx;
	line-height: 90rpx;
	font-size:14px;
	font-weight: 700;
	float:left;
	color:#6d6a6a;
	padding-left:30rpx;
}

.content_input .input{
	width: 70%;
	height:90rpx;
	padding-right:30rpx;
	font-size:13px;
	text-align: right;
}

.icon{
	width: 200rpx;
	height:200rpx;
	margin:0rpx auto;
	border-radius: 50%;
}

.submitData{
	width: 23%;
	height:70rpx;
	line-height: 70rpx;
	font-size:14px;
	background: white;
	margin:50rpx auto 0rpx;
}

.ts{font-size:20rpx;}

.switch{
	width: 60%;
	height:90rpx;
	padding-right:30rpx;
	font-size:13px;
	text-align: right;
}

.time1 {
  position: relative;
}

.right1 {
  width: 15%;
  height: 110rpx;
  position: absolute;
  right: -10rpx;
  top: 0rpx;
}

.right1 image {
  width: 40%;
  height: 45rpx;
  margin: 30rpx 0rpx 0rpx 33rpx;
}